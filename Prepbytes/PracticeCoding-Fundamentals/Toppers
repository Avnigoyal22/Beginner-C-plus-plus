#include <bits/stdc++.h>
using namespace std;

int main()
{
  int t;
  cin>>t;
  
  while(t--){
    int a,b,c,n;
    cin>>a>>b>>c>>n;
    
    int r=0;
    cout<<a<<" "<<b<<" "<<c<<" ";
    
    for(int i=4;i<=n;i++){
      r=a+b+c;
      cout<<r<<" ";
      a=b;
      b=c;
      c=r;
    }
    cout<<endl;
  }
  
  return 0;
}
